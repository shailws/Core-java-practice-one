//perform multiple task from multiple threads.

package MTdemo;

class Casee4 extends Thread {
	@Override
	public void start() {
		System.out.println("task 1");
	}
}

public class Case4 {
	public static void main(String[] args) {
		Casee4 t1 = new Casee4();
		t1.start();
		
		Casee4 t2 = new Casee4();
		t2.start();
		
		Casee4 t3 = new Casee4();
		t3.start();
	}
}
